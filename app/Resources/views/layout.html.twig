<!DOCTYPE html>
<html lang="fr">
<head>
    <meta charset="utf-8">

    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <meta name="description" content="Feed Your Tod">
    <meta name="author" content="Dave, Toufik, et David">

    <link rel="icon" href="{{ asset('public/images/ico.png') }}">
    <title>{% block title %}FeedYourTod{% endblock %}</title>

    {% block stylesheets %}
        <!-- css -->
        <link href="{{ asset('public/Sailor/css/bootstrap.min.css') }}" rel="stylesheet" />
        <link href="{{ asset('public/Sailor/css/cubeportfolio.min.css') }}" rel="stylesheet" />
        <link href="{{ asset('public/Sailor/css/style.css') }}" rel="stylesheet" />


        <!-- Theme skin -->
        <link id="t-colors" href="{{ asset('public/Sailor/skins/default.css') }}" rel="stylesheet" />

        <!-- boxed bg -->
        <link id="bodybg" href="{{ asset('public/Sailor/bodybg/bg1.css') }}" rel="stylesheet" type="text/css" />
    {% endblock %}



</head>
<body>





    <div id="wrapper">

        {{ include("::include/header.html.twig") }}




        {{ include("::include/headline_breadcrumb.html.twig") }}


        <section id="content">
            <div class="container">


                {% if app.session.started and app.session.flashBag.peekAll is not empty %}
                    <div class="messages">
                        {% for type, messages in app.session.flashBag.all %}
                            {% for message in messages %}
                                {# Bootstrap alert, see http://getbootstrap.com/components/#alerts #}
                                <div class="alert alert-dismissible alert-{{ type }}" role="alert">
                                    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                                        <span aria-hidden="true">&times;</span>
                                    </button>

                                    {{ message|trans }}
                                </div>
                            {% endfor %}
                        {% endfor %}
                    </div>
                {% endif %}

                {% block contentTitle %}
                    <h1>Bienvenue sur notre site internet!</h1>
                {% endblock %}

                {% block content %}{% endblock %}




            </div>
        </section>


        {{ include("::include/footer.html.twig") }}


    </div>
    <a href="#" class="scrollup"><i class="fa fa-angle-up active"></i></a>

    {% block javascripts %}
        <!-- javascript
            ================================================== -->
        <!-- Placed at the end of the document so the pages load faster -->
        <script src="{{ asset('public/Sailor/js/jquery.min.js') }}"></script>
        <script src="{{ asset('public/Sailor/js/modernizr.custom.js') }}"></script>
        <script src="{{ asset('public/Sailor/js/jquery.easing.1.3.js') }}"></script>
        <script src="{{ asset('public/Sailor/js/bootstrap.min.js') }}"></script>
        <script src="{{ asset('public/Sailor/js/jquery.appear.js') }}"></script>
        <script src="{{ asset('public/Sailor/js/stellar.js') }}"></script>
        <script src="{{ asset('public/Sailor/js/classie.js') }}"></script>
        <script src="{{ asset('public/Sailor/js/uisearch.js') }}"></script>
        <script src="{{ asset('public/Sailor/js/jquery.cubeportfolio.min.js') }}"></script>
        <script src="{{ asset('public/Sailor/js/google-code-prettify/prettify.js') }}"></script>
        <script src="{{ asset('public/Sailor/js/animate.js') }}"></script>
        <script src="{{ asset('public/Sailor/js/custom.js') }}"></script>
    {% endblock %}

</body>
</html>